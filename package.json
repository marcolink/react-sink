{
  "name": "react-sink",
  "version": "0.0.0",
  "type": "module",
  "author": "Marco Link",
  "license": "MIT",
  "repository": "marcolink/react-sink",
  "homepage": "https://github.com/marcolink/react-sink",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/react-sink.umd.js",
  "module": "./dist/react-sink.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-sink.es.js",
      "require": "./dist/react-sink.umd.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "presemantic-release": "yarn run build",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.0.4",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^21.0.5",
    "typescript": "^5.0.4",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.34.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github"
    ]
  }
}
